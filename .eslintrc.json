{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:mocha/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["mocha", "@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "warn"
  }
}
